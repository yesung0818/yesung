#include <stdio.h>
int main(){
            int i,num,sum=0;

            printf("정수 입력 :");
            scanf("%d",&num);

            for(i=0;i<=num;i++)
                        if(i%5==0)
                        sum=sum+i;

            printf("1부터 %d까지 5의 배수의 합 : %d",num,sum);
           return 0;
                       }
